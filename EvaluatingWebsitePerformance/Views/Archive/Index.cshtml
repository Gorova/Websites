@model IEnumerable<EvaluatingWebsitePerformance.ViewModel.WebsiteViewModel>
@{
    ViewBag.Title = "choose site for analyzing of performance";
}

<h2>@ViewBag.Title</h2>

<table class="table table-hover">
    <tr class="active">
        <th>
            @Html.DisplayNameFor(model => model.Url)
        </th>
        <th>
            @Html.DisplayName("operations")
        </th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Url)
            </td>
            <td>
                <button class="btn btn-info" onclick="getList(@item.Id)"> build graph</button>

            </td>
        </tr>
    }
</table>
<canvas id="myChart" width="600" height="400"></canvas>


